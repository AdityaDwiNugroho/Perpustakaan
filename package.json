{
  "name": "perpustakaan-nuxxt-app",
  "description": "Aplikasi perpustakaan sederhana dengan Nuxt.js dan Prisma ORM",
  "version": "1.0.5",
  "release": {
    "version": "1.0.5",
    "date": "2025-02-15"
  },
  "license": "MIT",
  "author": {
    "name": "Aditya Dwi Nugroho",
    "email": "aditadit120420@gmail.com",
    "url": "https://adityaa.me",
    "github": "https://github.com/AdityaDwiNugroho"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AdityaDwiNugroho/UKKPerpus"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "npm run seed:all"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "seed:all": "tsx server/tasks/user-seed.ts && tsx server/tasks/book-seed.ts"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@prisma/nuxt": "^0.3.0",
    "@sidebase/nuxt-auth": "^0.10.0",
    "bcrypt": "^5.1.1",
    "next-auth": "~4.21.1",
    "nuxt": "^3.16.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@iconify/vue": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.13.10",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite-plugin-vuetify": "^2.1.0",
    "vuetify": "^3.7.16"
  }
}
